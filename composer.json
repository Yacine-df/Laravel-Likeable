{
    "name": "yacinediaf/laravel-likeable",
    "description": "Laravel package that offers the functionality of liking different type of models e.g like post, comment, video,..",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yacinediaf\\LaravelLikeable\\": "src/",
            "Yacinediaf\\LaravelLikeable\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Yacine-df",
            "email": "ycn.diaf@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^11.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "orchestra/testbench": "^9.3"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel":{
            "providers":[
                "Yacinediaf\\LaravelLikeable\\ServiceProvider"
            ]
        }
    }
}
